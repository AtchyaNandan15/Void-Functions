// writing the first function


//declares the function 
void Moon(void);



main()
{
//call the moon function
   Moon(); 
    
}


//writing the function
void Moon(void){

    cout<<"This is the moon in the galaxy by christopher Johnson \n Atchya Nandan";
  
    cout<<"                             ___       .                       \n"; 
    cout<<" .                        _.--~~ __-.                          \n";
    cout<<"                     ,- +     .-~  ~-}}              .         \n";
    cout<<"                  ...     -------------     .                  \n";
    cout<<"                 +     _(         ~                            \n";
    cout<<"                /      Y                            .          \n";
    cout<<"               /     (}}                     +                 \n";
    cout<<"   .          Y     (__                                        \n";
    cout<<"             |      (                                          \n";    
    cout<<"            |      (___                                        \n";
    cout<<"           .        |                    .                     \n";
    cout<<"            l      _)                                          \n";
    cout<<" .          ||      l                                          \n";
