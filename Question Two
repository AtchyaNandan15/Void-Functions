#include <iostream>

using namespace std;

void DrawOne(void);
void DrawTwo(void);
void DrawThree(void);

int main()
{
    
    int user;

cout<<"Enter the number 1,2 or 3";
cin>>user;
if(user==1){
DrawOne();
}
else if (user==2){
DrawTwo();
}

else if (user==3){
DrawThree();
}



}



void DrawOne (void) {
    
    cout<<"{:)}";
    
} 



    
    void DrawTwo (void) {
    
    cout<<"{:=}";
    
} 


    
    void DrawThree (void) {
    
    cout<<"{:*}";
    
} 



