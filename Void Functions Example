#include <iostream>
#include <string>
using namespace std;

//declare a function that puts out a greeting
void helloWorld(void);
void byeWorld(void);


int main()
{
 
//call hello world function    
helloWorld();
byeWorld();
}


void byeWorld(void){
    cout<<"\nBye World\n";  
}


//writing the function
void helloWorld(void){
     cout<<"Hello world\n";
}

